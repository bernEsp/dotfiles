# This is Git's per-user configuration file.
[user]
  name = Bernardo Galindo
  email = bernardo466@gmail.com
[init]
  templatedir = ~/.git
[alias]
  ctags = !.git/hooks/ctags
  mylog = log --pretty=format:'%h %s [%an]' --graph
  lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
  cherrylog = log --graph --left-right --cherry-pick --oneline
  changelog = log --graph --oneline --pickaxe-all --patch
  wc = whatchanged -p
  st = status
  co = checkout
  br = branch
  ci = commit
[color]
  ui = true
[core]
  editor = /usr/bin/vim -f
  excludesfile = /Users/bernardogalindo/.gitignore
[web]
  browser = open
